{
  "name": "Typifier",
  "version": "1.2.15",
  "description": "Transpiler for convertion JavaScript code to Typescript in kuraCloud",
  "main": "./distr/main.js",
  "scripts": {
    "test": "jest",
    "testMon": "nodemon --watch src --exec jest",
    "watch": "webpack --watch --colors --progress",
    "make": "webpack --colors --progress",
    "start": "electron distr/main.js",
    "debug": "nodemon --exec electron distr/main.js --dev",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "Toby Suggate <t.suggate@adinstruments.com>",
  "license": "ISC",
  "build": {
    "appId": "com.tsuggate.typify",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "category": "Utility"
    },
    "nsis": {
      "perMachine": true
    },
    "fileAssociations": [
      {
        "ext": "js",
        "role": "Editor"
      }
    ],
    "files": [
      "distr",
      "node_modules",
      "test-files",
      "resources"
    ]
  },
  "dependencies": {
    "acorn": "https://registry.npmjs.org/acorn/-/acorn-5.1.1.tgz",
    "commander": "^2.9.0",
    "css-loader": "^0.28.0",
    "electron-window-state": "^4.1.1",
    "escodegen": "^1.8.1",
    "esprima": "^4.0.0",
    "estree-walker": "^0.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "fs-extra": "^2.1.2",
    "js-beautify": "^1.6.12",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "nodemon": "^1.10.2",
    "react": "^15.5.4",
    "react-ace": "^5.1.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "style-loader": "^0.16.0",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "underscore": "^1.8.3",
    "webpack": "^2.4.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/react-redux": "^5.0.10",
    "@types/commander": "^2.9.0",
    "@types/diff": "^3.2.0",
    "@types/escodegen": "0.0.6",
    "@types/esprima": "^2.1.33",
    "@types/estree": "0.0.34",
    "@types/fs-extra": "^2.1.0",
    "@types/jest": "^20.0.5",
    "@types/js-beautify": "0.0.29",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/underscore": "^1.7.36",
    "@types/winston": "^2.3.3",
    "diff": "^3.2.0",
    "electron": "^1.6.2",
    "electron-builder": "^17.5.0",
    "jest": "^20.0.4",
    "jest-teamcity": "^1.3.0",
    "ts-jest": "^20.0.7"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coverageDirectory": "<rootDir>/coverage~~",
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx,js}"
    ],
    "mapCoverage": true,
    "testResultsProcessor": "jest-teamcity"
  }
}
